clear all
close all
clc
n = 1;
m = 1;

for k = 1:2:10
    transferencia(n)=modelo2neuronas4AP(k,30,4e6,1e-2,0);
    frecuencias(n) = k;
    n = n + 1
end

for k = 10:20:100
    transferencia(n)=modelo2neuronas4AP(k,200,4e6,1e-2,0);
    frecuencias(n) = k;
    n = n + 1
end

for k = 100:100:600
    transferencia(n)=modelo2neuronas4AP(k,100,4e6,1e-3,0);
    frecuencias(n) = k;
    n = n + 1
end

frecuencias = frecuencias*1e3;

figure(5)
semilogx(frecuencias,transferencia);
